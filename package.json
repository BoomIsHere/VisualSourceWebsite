{
  "name": "vs_webserver",
  "version": "3.0.0",
  "description": "visualsource web server",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "nodemon -x ts-node --files ./src/server.ts",
    "test": "jest",
    "build": "tsc --sourceMap -p ./",
    "start": "node ./dist/server.js"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "env": {
      "DEV": false,
      "PORT": 8000
    }
  },
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "keywords": [],
  "author": "Collin",
  "license": "MIT",
  "repository": {
    "type": "Github",
    "url": "https://github.com/BoomIsHere/VisualSourceWebsite"
  },
  "dependencies": {
    "fastify": "^2.11.0",
    "fastify-auth": "0.7.2",
    "fastify-cookie": "^3.5.0",
    "fastify-cors": "^3.0.1",
    "fastify-helmet": "^3.0.2",
    "fastify-plugin": "^1.6.0",
    "fastify-static": "^2.5.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "7.17.1",
    "knex": "^0.20.8",
    "request": "^2.88.0",
    "uuid": "^3.4.0",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
