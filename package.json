{
  "name": "vs_webserver",
  "version": "3.5.0",
  "description": "visualsource web server",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "nodemon -x ts-node --files ./src/server.ts",
    "test": "jest",
    "build": "tsc --sourceMap -p ./",
    "start": "node ./dist/server.js",
    "internet":"./test/ngrok http 8000"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "env": {
      "production": false,
      "PORT": 8000,
      "DEBUG": "socket.io:*"
    }
  },
  "engines": {
    "node": "14.4.0",
    "npm": "6.14.5"
  },
  "keywords": [],
  "author": "Collin",
  "license": "MIT",
  "repository": {
    "type": "Github",
    "url": "https://github.com/BoomIsHere/VisualSourceWebsite"
  },
  "dependencies": {
    "express-jwt": "^6.0.0",
    "fastify": "^2.15.1",
    "fastify-auth": "1.0.1",
    "fastify-cookie": "^4.0.2",
    "fastify-cors": "^3.0.3",
    "fastify-helmet": "^5.0.1",
    "fastify-plugin": "^1.6.1",
    "fastify-static": "^3.2.0",
    "jwks-rsa": "^1.8.1",
    "knex": "^0.21.1",
    "pg": "8.3.2",
    "random-js": "^2.1.0",
    "simplex-noise": "^2.4.0",
    "socket.io": "^2.3.0",
    "three": "^0.115.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.11.8",
    "@types/socket.io": "^2.1.8",
    "jest": "^24.9.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
